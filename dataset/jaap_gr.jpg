<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />

<title>Egelopvang.nl - de thuisbasis van egelopvang sites in Nederland</title>

<meta name="robots" content="index,follow,noodp,noydir" />
<meta name="description" content="Egelopvang.nl geeft een overzicht van alle egelopvang sites in Nederland." />

<link rel="stylesheet" type="text/css" href="css/main.css">

<!--<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=API_KEY&sensor=false"></script>-->
<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?v=3.exp&amp;sensor=false"></script>
<script type="text/javascript">
	function initialize() {
		//var hilversum= new google.maps.LatLng(52.229170,5.166897);
		//var soest= new google.maps.LatLng(52.176352,5.299197);
		//var amersfoort= new google.maps.LatLng(52.156111,5.387827);
		
		//set map and centre it
		var baarn= new google.maps.LatLng(52.213182,5.286410);
		var mapOptions = {
			zoom: 8,
			center: baarn,
			mapTypeId: google.maps.MapTypeId.TERRAIN
		};		
		var map = new google.maps.Map(document.getElementById("map-canvas"), mapOptions);

		//special image for Zoetermeer
		var zmeer= new google.maps.LatLng(52.035909,4.489443);
		var homeImgObj = {
			url: 'img/animal-shelter-export.png',
			// This marker is 32 pixels wide by 37 pixels tall.
			size: new google.maps.Size(32, 37),
			// The origin for this image is 0,0.
			origin: new google.maps.Point(0,0),
			// The anchor for this image is the base of the flagpole at 0,30.
			anchor: new google.maps.Point(16, 45)
        };
		var mzmeer = new google.maps.Marker({
			position: zmeer,
			map: map,
			title: 'Egelopvang Zoetermeer',
			icon: homeImgObj,
			url: 'http://www.egelopvang.nl/zoetermeer'
		});

		setMarkers(map, centra);
		
		google.maps.event.addListener(mzmeer, 'click', function() { window.location.href = mzmeer.url; });
		}
		
		//set markers on the map
		//centra array: title, latitude, longitude, url
		var centra = [
			['Faunavisie Wildcare', 53.400080, 6.478902, 'http://www.faunavisie.nl/'],
			['Stichting Egelopvang Nieuwleusen', 52.569856, 6.260366, 'http://www.stichtingegelopvangnieuwleusen.nl/'],
			['Stichting Dierenopvangcentrum Groningen', 53.253640, 6.543735, 'http://www.dierenopvangcentrum-groningen.nl/index.htm'],
			['De Fugelhelling', 53.105368, 6.139394, 'http://www.defugelhelling.nl'],
			['\'t Egelhuus', 52.708516, 6.540871, 'http://www.egelhuus.nl'],
			['Egelopvang Caspar', 52.289698, 6.153500, 'http://www.egelopvangcaspar.nl'],
			['Stichting Egelbescherming', 52.302492, 5.147570, 'http://www.egelbescherming.nl'],
			['Haarlemse Egelopvang', 52.376975, 4.618446, 'http://www.egelopvanghaarlem.nl'],
			['De Bonte Piet', 52.716219, 5.027855, 'http://www.debontepiet.com'],
			['De Dikke Prik', 52.710231, 4.717532, 'http://www.dikkeprik.com'],
			['Egelopvang Den Haag', 52.075187, 4.235448, 'http://www.egelopvangdenhaag.nl'],
			['Egelopvang Westland', 52.005618, 4.246184, 'http://www.egelopvang-westland.nl'],
			['Vogel- en Egelopvang Delft', 52.015291, 4.377065, 'http://www.vogelopvangdelft.nl'],
			['Stichting Opvang Noach', 51.999436, 6.413411, 'http://www.opvangnoach.nl/'],
			['Roofvogel, uilen en egelopvang', 51.887425, 5.382613, 'http://www.roofvogelopvang.nl/'],
			['Stichting Egelopvang Roosendaal', 51.523194, 4.496063, 'http://www.egelopvangroosendaal.nl/'],
			['Egels en Co', 51.374859, 6.114393, 'http://www.egelsenco.nl/'],
			['Egelopvang Zuid-Holland Zuid', 51.827653, 4.695059, 'http://www.egelopvangZHZ.nl'],
			['Egelopvang Zuid Limburg', 51.027843, 5.815389, 'http://www.dierenhulpcentrum.nl/index.php/de-egelopvang'],
			['Vogelrevalidatiecentrum Zundert', 51.4879896,4.6589362, 'http://www.vrczundert.nl']
        ];
		function setMarkers(map, locations) {
			var image = {
				url: 'img/WhiteHedgehogIcon.png',
				// This marker is 28 pixels wide by 26 pixels tall.
				size: new google.maps.Size(28, 26),
				// The origin for this image is 0,0.
				origin: new google.maps.Point(0,0),
				// The anchor for this image is the base of the flagpole at 0,30.
				anchor: new google.maps.Point(14, 13)
			};
			
			for (var i = 0; i < locations.length; i++) {
				var centrum = locations[i];
				var myLatLng = new google.maps.LatLng(centrum[1], centrum[2]);
				var marker = new google.maps.Marker({
					position: myLatLng,
					map: map,
					title: centrum[0],
					url: centrum[3],
					icon: image
				});
				(function (i, marker) {
					  google.maps.event.addListener(marker, 'click', function () {
						  window.location.href = marker.url;});
				})(i, marker);
			}
        }
	google.maps.event.addDomListener(window, 'load', initialize);
</script>
       
</head>


<body>

